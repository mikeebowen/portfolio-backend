{
  "name": "portfolio-backend",
  "version": "0.0.1",
  "description": "the backend for my portfolio website",
  "main": "server.js",
  "scripts": {
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "docs": "./node_modules/.bin/rimraf out_apidoc/ out/ && ./node_modules/.bin/apidoc -i api/ -o out_apidoc/ && ./node_modules/.bin/jsdoc api/ lib/ -r README.md",
    "start": "node server.js",
    "test": "nyc ./node_modules/.bin/mocha --recursive api/**/*.test.js lib/**/*.test.js local_modules/**/*.test.js && nyc report --reporter=text-lcov | coveralls -service = travis-ci",
    "mongod": "./node_modules/.bin/mongod --dbpath=./data",
    "dev": "nodemon"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mrbgit/portfolio-backend.git"
  },
  "author": "Michael Bowen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mrbgit/portfolio-backend/issues"
  },
  "homepage": "https://github.com/mrbgit/portfolio-backend#readme",
  "dependencies": {
    "body-parser": "^1.17.2",
    "cli-color": "^1.2.0",
    "compression": "^1.7.0",
    "config": "^1.26.1",
    "cors": "^2.8.4",
    "express": "^4.15.3",
    "file-type": "^5.2.0",
    "moment": "^2.18.1",
    "mongodb": "^2.2.30",
    "mongoose": "^4.11.4",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "@types/cli-color": "^0.3.29",
    "@types/express": "^4.0.36",
    "apidoc": "^0.17.6",
    "chai": "^4.1.0",
    "coveralls": "^2.13.1",
    "eslint": "^4.3.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "jsdoc": "^3.5.3",
    "mocha": "^3.4.2",
    "mockgoose": "^7.3.3",
    "nyc": "^11.1.0",
    "proxyquire": "^1.8.0",
    "rimraf": "^2.6.1",
    "sinon": "^2.4.1"
  }
}
